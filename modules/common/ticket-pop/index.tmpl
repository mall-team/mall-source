<div class="ticket-container">
	<ul>
		<% $.each(list, function(i, item){ %>
		<li>
			<div class="money"><i>&yen;</i><b><%= item.coupon_money %></b></div>
			<div class="ticket-info">
				<p class="ticket-title"><%= item.title %></p>
				<% if(item.order_money==0){ %>
				<!-- <p class="des">全场通用</p> -->
				<% }else{ %>
				<p class="des">满<%= item.order_money %>可用</p>
				<% } %>
			</div>
			<a class="radio <%= curId==item.coupon_code?'selected':'' %> dib" ticket-item='<%= JSON.stringify(item) %>'><i class="icon-radio"></i></a>
		</li>
		<% }) %>
		<li>
			<div class="ticket-info">不使用优惠券</div>
			<a class="radio dib" ticket-item="-1"><i class="icon-radio"></i></a>
		</li>
	</ul>
</div>